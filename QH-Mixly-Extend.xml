<!-- 
  type="company" 
  block="block/qhMixlyExtension.js" 
  generator="generator/qhMixlyExtension.js" 
  color="color/qhMixlyExtension.js" 
  lib="qheduino" 
  media="media/qheduino"
  language="language/qheduino" 
